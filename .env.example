# Format: InstrumentationKey=xxxxx;IngestionEndpoint=https://...
APPLICATIONINSIGHTS_CONNECTION_STRING=your-connection-string-here

# Environment
NODE_ENV=production

# Port (optionnel, par défaut 8080)
PORT=8080
